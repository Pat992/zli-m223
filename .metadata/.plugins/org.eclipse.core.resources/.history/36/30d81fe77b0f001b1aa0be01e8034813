@Entity(name="Customer")
public class CustomerImpl implements Customer {

	@Id
	@GeneratedValue
	private Long id;
	
	private String name;
	private String street;
	private String city;
	
	protected CustomerImpl() {} // For Jpa
	
	protected CustomerImpl(String name, String street, String city) {
		this.name = name;
		this.street = street;
		this.city = city;
	}
	
	@Override
	public Long getId() {
		return this.id;
	}

